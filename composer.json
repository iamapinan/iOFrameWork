{
    "name": "iotech/ioframework",
    "type": "library",
    "description": "Easy to use PHP Framework MVC Model",
    "keywords": [
        "PHP Framework",
        "mvc",
        "framework",
        "laravel",
        "io",
        "iotech",
        "easy php"
    ],
    "homepage": "http://iotech.co.th/iOFramework",
    "authors": [
        {
            "name": "Apinan Woratrakun",
            "email": "apinan@iotech.co.th",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "catfan/Medoo": "1.5.6",
        "nezamy/route": "1.2.2",
        "gilbitron/easycsrf": "1.0.1",
        "vlucas/phpdotenv": "2.4.0",
        "robmorgan/phinx": "0.10.3",
        "phpunit/phpunit": "6.5.5",
        "php-school/cli-menu": "2.1.0",
        "jenssegers/blade": "1.1.0"
    },
    "require-dev": {
        "filp/whoops": "2.1.14"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "IO\\Framework\\": "src/",
            "\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iOTest\\": "tests/"
        }
    },
    "support": {
        "email": "developer@iotech.co.th"
    }
}
