{
    "name": "iotech/ioframework",
    "type": "library",
    "description": "Easy to use PHP Framework MVC Model",
    "keywords": [
        "PHP Framework",
        "mvc",
        "framework",
        "laravel",
        "io",
        "iotech",
        "easy php"
    ],
    "homepage": "http://iotech.co.th/iOFramework",
    "authors": [
        {
            "name": "Apinan Woratrakun",
            "email": "apinan@iotech.co.th",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://git.iotech.co.th/iotech/iOFramework.git"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "catfan/Medoo": "1.5.6",
        "nezamy/route": "dev-master",
        "dwoo/dwoo": "1.3.7",
        "gilbitron/easycsrf": "1.0.1",
        "vlucas/phpdotenv": "2.4.0",
        "robmorgan/phinx": "dev-master",
        "phpunit/phpunit": "7.2.6"
    },
    "require-dev": {
        "filp/whoops": "2.1.14",
        "maximebf/consolekit": ">=1.0.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "iO\\Framework\\": "src/",
            "\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iOTest\\": "tests/"
        }
    },
    "support": {
        "email": "developer@iotech.co.th"
    }
}
